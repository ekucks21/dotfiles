{
    "git.path": "C:\\cygwin64\\usr\\bin\\git.exe",
    "workbench.colorTheme": "Quiet Light",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "scrollkey.line1": 1,
    "scrollkey.line2": 30,
    "scrollkey.line3": 60,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": ["f","d"],
            "after": ["<Esc>"]
        }
    ],
    "vim.otherModesKeyBindingsNonRecursive": [
        {
            "before": ["<leader>","d"],
            "after": ["d", "d"]
        },
        {
            "before": ["g", "D"],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.goToTypeDefinition",
                    "args": []
                }
            ]
        },
        {
            "before": ["<leader>", "r"],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.rename",
                    "args": []
                }
            ]
        },
        {
            "before": ["<leader>", "f", "r"],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.referenceSearch.trigger",
                    "args": []
                }
            ]
        },
        {
            "before": ["g", "p"],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.previewDeclaration"
                }
            ]
        },
        {
            "before": ["<leader>", "e"],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.marker.next",
                    "args": []
                }
            ]
        },
        {
            "before": ["<C-S-]>"],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.goToImplementation"
                }
            ]
        },
        {
            "before": ["<C-]>"],
            "after": [],
            "commands": [
                {
                    "command": "editor.action.goToDeclaration"
                }
            ]
        },
        {
            "before": ["<C-i>"],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.navigateForward"
                }
            ]
        },
        {
            "before": ["<C-o>"],
            "after": [],
            "commands": [
                {
                    "command": "workbench.action.navigateBack"
                }
            ]
        },
        {
            "before":["<C-n>"],
            "after":[],
            "commands": [
                {
                    "command": ":nohl"
                }
            ]
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys":{
        "<C-a>": false,
        "<C-f>": false
    },
    "files.autoSave": "afterDelay",
    "window.zoomLevel": -1
}
